<?xml version="1.0" encoding="UTF-8"?>

<endpoints xmlns='http://java.sun.com/xml/ns/jax-ws/ri/runtime' version='2.0'>

    <endpoint
            name='HelloWorld'
            implementation='com.ws.examples.HelloWorld'
            url-pattern='/ws/Hello'/>
    <endpoint
            name='SoapServiceRPC'
            implementation='com.ws.rpc.SoapServiceRPC'
            url-pattern='/ws/rpc'/>
    <endpoint name="SoapServiceDocumentBare"
              implementation="com.ws.document.SoapDocumentBare"
              url-pattern="/ws/bare" />
    <endpoint name="SoapServiceDocumentWrap"
              implementation="com.ws.document.SoapDocumentWrapped"
              url-pattern="/ws/wrap" />
</endpoints>
